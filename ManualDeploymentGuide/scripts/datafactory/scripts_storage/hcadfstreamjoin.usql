//-------------------------------------------------------------------------------------------
// Query the severity file for the past some minutes worth of data (determined by @queryLength)
//-------------------------------------------------------------------------------------------
@severityResult = 
    SELECT 
        *
     FROM @severityInput
     WHERE date > @fileFormatTime AND recordTime >= @startQuery AND recordTime <= @endQuery; 

//------------------------------------------------------------------------------------------
// Query the changes file for the past some minutes worth of data (determined by @queryLength)
//-------------------------------------------------------------------------------------------
@changesResult = 
    SELECT 
        *
     FROM @changesInput
     WHERE date > @fileFormatTime AND recordTime >= @startQuery AND recordTime <= @endQuery; 

//-----------------------------------------------------------------------------------------
// Query the core file for the past some minutes worth of data (determined by @queryLength)
//-----------------------------------------------------------------------------------------
@coreResult = 
    SELECT 
        *
     FROM @coreInput
     WHERE date > @fileFormatTime AND recordTime >= @startQuery AND recordTime <= @endQuery; 

//-----------------------------------------------------------------------------------------
// Query the DXPR file for the past some minutes worth of data (determined by @queryLength)
//-----------------------------------------------------------------------------------------
@dxprResult = 
    SELECT 
        *
     FROM @dxprInput
     WHERE date > @fileFormatTime AND recordTime >= @startQuery AND recordTime <= @endQuery; 
	 
