// hcadfstreamscore.usql
// Script summary:
// This usql script is used to submit ADLA job that constitutes the second activity in ADF
// In this usql script, we will extract the joined data and do Feature Engineering using usql.
// Then we will pass the Feature Engineered data to an R script, which will return the Predcitions
// Finally we will join the predictions to the original data in usql.
// We will deploy the R script that does the scoring - phm_adf_score_streamed.R and 
// the pretrained models "hosp_1_LOSmodel.rds", "hosp_1_LOSmodel.rds", etc. in the usql scrpit



REFERENCE ASSEMBLY [ExtR];

//for row based operations, we can choose to split the data (DoBalancedComputing_Flag=true) among PartitionCount workers
DECLARE @DoBalancedComputing_Flag bool = false;
DECLARE @PartitionCount int = 10;


//deploy R scripts - "phm_adf_score_streamed.R" 
DECLARE @path_Rscipt string = @"/adfRscripts" ;   
DECLARE @RScriptName string = @"phm_score_streamed.R";  
DECLARE @RScriptFullName string = @path_Rscipt + @"/" + @RScriptName;
DEPLOY RESOURCE @RScriptFullName;

//deploy schema file with column names and R class/type info - "R_schema_with_data_type_phm_data.csv"
//DECLARE @path_meta string  = @"/historic_meta/" ;
//DECLARE @SchemaFile string = @path_meta + @"R_schema_with_data_type_phm_data.csv";
//DEPLOY RESOURCE @SchemaFile;


//deploy scoring models, hospital specific and one generic
DECLARE @path_models string = "/models/";

DECLARE @model00 string = @path_models  + @"allotherhosp_LOSmodel.rds";
DECLARE @model01 string = @path_models  + @"hosp_1_LOSmodel.rds";
DECLARE @model02 string = @path_models  + @"hosp_2_LOSmodel.rds";
DECLARE @model03 string = @path_models  + @"hosp_3_LOSmodel.rds";
DECLARE @model04 string = @path_models  + @"hosp_4_LOSmodel.rds";
DECLARE @model05 string = @path_models  + @"hosp_5_LOSmodel.rds";
DECLARE @model06 string = @path_models  + @"hosp_6_LOSmodel.rds";
DECLARE @model07 string = @path_models  + @"hosp_7_LOSmodel.rds";
DECLARE @model08 string = @path_models  + @"hosp_8_LOSmodel.rds";
DECLARE @model09 string = @path_models  + @"hosp_9_LOSmodel.rds";
DECLARE @model10 string = @path_models  + @"hosp_10_LOSmodel.rds";

DEPLOY RESOURCE @model00;
DEPLOY RESOURCE @model01;
DEPLOY RESOURCE @model02;
DEPLOY RESOURCE @model03;
DEPLOY RESOURCE @model04;
DEPLOY RESOURCE @model05;
DEPLOY RESOURCE @model06;
DEPLOY RESOURCE @model07;
DEPLOY RESOURCE @model08;
DEPLOY RESOURCE @model09;
DEPLOY RESOURCE @model10;

